{
  "name": "@eivmosn/utils",
  "type": "module",
  "version": "1.0.6",
  "packageManager": "pnpm@8.8.0",
  "description": "Common util collections for javascript and typescript",
  "author": "eivmosn <eivmosn@163.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eivmosn/utils.git"
  },
  "bugs": "https://github.com/eivmosn/utils/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "release": "bumpp && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "prepublishOnly": "nr build",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks",
    "up": "taze major"
  },
  "dependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/throttle-debounce": "^5.0.2",
    "@vue/shared": "^3.3.11",
    "rollup": "^4.7.0",
    "rollup-plugin-dts": "^6.1.0",
    "throttle-debounce": "^5.0.0",
    "tslib": "^2.6.2",
    "tsup": "^8.0.1",
    "unbuild": "^2.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.4.3",
    "@types/node": "^20.10.4",
    "bumpp": "^9.2.0",
    "eslint": "^8.55.0",
    "esno": "^4.0.0",
    "lint-staged": "^15.2.0",
    "pnpm": "^8.12.0",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.7",
    "vitest": "^1.0.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
